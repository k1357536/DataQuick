{
  "name": "DataQuick",
  "version": "0.0.1",
  "description": "DataQuick",
  "scripts": {
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:watch": "concurrently \"npm run build:client:watch\" \"npm run build:server:watch\"",
    "build:client": "tsc -p client/",
    "build:server": "tsc -p server/",
    "build:client:watch": "tsc -p client/ -w",
    "build:server:watch": "tsc -p server/ -w",
    "serve": "nodemon server/server.js -watch server -watch client",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "Michael Hansal",
  "license": "UNLICENSED",
  "dependencies": {
    "@angular/common": "^4.4.0-RC.0",
    "@angular/compiler": "^4.4.0-RC.0",
    "@angular/core": "^4.4.0-RC.0",
    "@angular/forms": "^4.4.0-RC.0",
    "@angular/http": "^4.4.0-RC.0",
    "@angular/platform-browser": "^4.4.0-RC.0",
    "@angular/platform-browser-dynamic": "^4.4.0-RC.0",
    "@angular/router": "^4.4.0-RC.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.4",
    "angular-in-memory-web-api": "^0.3.0",
    "core-js": "^2.4.1",
    "express": "^4.15.4",
    "morgan": "^1.8.2",
    "rxjs": "^5.0.1",
    "systemjs": "^0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.46",
    "@types/systemjs": "^0.20.5",
    "concurrently": "^3.2.0",
    "nodemon": "^1.12.0",
    "tslint": "^3.15.1",
    "typescript": "~2.1.0"
  },
  "repository": {}
}
